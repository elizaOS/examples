{
  "name": "@elizaos/example-discord",
  "version": "2.0.0-alpha",
  "description": "Full-featured Discord AI agent using elizaOS",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run agent.ts",
    "dev": "bun --watch run agent.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "bunx @biomejs/biome check --write --unsafe .",
    "lint:check": "bunx @biomejs/biome check ."
  },
  "dependencies": {
    "@elizaos/core": "workspace:*",
    "@elizaos/plugin-discord": "workspace:*",
    "@elizaos/plugin-openai": "workspace:*",
    "@elizaos/plugin-sql": "workspace:*",
    "dotenv": "^17.2.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "@types/uuid": "^11.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
