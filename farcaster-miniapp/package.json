{
    "name": "eliza-multichain-miniapp",
    "version": "1.0.0",
    "type": "module",
    "description": "ElizaOS Multi-chain Trading Platform - Farcaster Mini App",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "start": "node server.js",
        "test": "vitest",
        "test:real": "USE_REAL_API=true vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "vitest run __tests__/integration"
    },
    "dependencies": {
        "@farcaster/miniapp-sdk": "^0.2.1",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "react": "^19.2.3",
        "react-dom": "^19.2.3"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.5.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/react": "^19.2.3",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.17",
        "@vitest/ui": "^4.0.17",
        "jsdom": "^26.0.0",
        "typescript": "^5.9.3",
        "vite": "^6.0.1",
        "vitest": "^4.0.17"
    }
}